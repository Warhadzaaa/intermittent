<div class="container-formulaire">
  <h2>Edit project</h2>
  <div class="formulaire">
    <div>
      <%= simple_form_for [@company, @project] do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :start_date %>
        <%= f.input :end_date %>
        <%= f.button :submit, class: "button" %>
      <% end %>
    </div>
  </div>
</div>
